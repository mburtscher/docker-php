{
    "name": "mburtscher/docker-php",
    "license": "MIT",
    "type": "library",
    "description": "A Docker PHP client",
    "autoload": {
        "psr-4": {
            "Docker\\": "src/",
            "Docker\\API\\": "generated/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/filesystem": "^3.0",
        "symfony/process": "^3.0",
        "jane/openapi-runtime": "^2.0",
        "php-http/socket-client": "^1.3",
        "php-http/client-common": "^1.5",
        "php-http/message": "^1.6",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "jane/open-api": "^3.0",
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-clover build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.31-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
